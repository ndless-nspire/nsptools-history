/* See http://sourceware.org/binutils/docs-2.20/ld/Scripts.html#Scripts */

/* Avoid the useless default padding between sections */
SECTIONS
{
  . = 0x0;
  .text : { *(.text) }
  .data : { *(.data) }
  .bss : { *(.bss) }
  /* required by newlib */
  __bss_start__ = .;
  __bss_end__ = . ;
}
